{
    "name": "ministryofjustice/opg-lpa-api",
    "description": "API service for creating Lasting Power of Attorney PDF documents",
    "minimum-stability": "stable",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-logger"
        }
    ],
    "require": {
        "php": "7.0.*",
        "aws/aws-sdk-php": "^3.0",
        "guzzlehttp/guzzle": "^6.0",
        "ministryofjustice/opg-lpa-logger": "^3.0",
        "ministryofjustice/opg-lpa-datamodels": "^9.0",
        "nsmithuk/dynamo-queue-php": "^1.0",
        "mongodb/mongodb": "^1.2",
        "zendframework/zend-component-installer": "^1.1",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-paginator": "^2.8",
        "zf-commons/zfc-rbac": "^2.6",
        "zfcampus/zf-api-problem": "^1.2",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-crypt": "^3.2",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-console": "^2.6",
        "nocarrier/hal": "^0.9.12",
        "zendframework/zend-mvc-console": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/Application/"
        },
        "files":  [
            "module/Application/Module.php"
        ]
    }
}
